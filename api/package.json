{
  "name": "@bananasgear/api",
  "version": "0.0.1",
  "main": "serverless.ts",
  "scripts": {
    "deploy:production": "pnpm run typecheck && pnpm test:unit && sls deploy --aws-profile goalflags --stage pro",
    "deploy:test": "pnpm run typecheck && pnpm test:unit && sls deploy --aws-profile goalflags --stage test",
    "deploy:test:function": "sls deploy function --stage test -f",
    "deploy:test:debug-mode": "sls deploy --aws-profile goalflags --stage test",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "install:dynamodb-local": "sls dynamodb install",
    "lint": "eslint \"**/*.{ts,js}\"",
    "remove:test": "sls remove --aws-profile goalflags --stage test",
    "start:dynamodb-local": "sls dynamodb start",
    "tdd": "vitest --config ./vitest.config.ts",
    "test:unit": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@serverless/typescript": "^3.21.0",
    "@types/aws-lambda": "^8.10.101",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.6.3",
    "esbuild": "^0.14.51",
    "eslint": "^8.21.0",
    "json-schema-to-ts": "^2.5.4",
    "prettier": "^2.7.1",
    "serverless-esbuild": "^1.31.0",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-pseudo-parameters": "^2.6.1",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4",
    "vitest": "^0.20.2"
  },
  "dependencies": {
    "@middy/core": "^2.5.7",
    "@middy/http-cors": "^2.5.7",
    "@middy/http-error-handler": "^2.5.7",
    "@middy/http-event-normalizer": "^2.5.7",
    "@middy/http-json-body-parser": "^2.5.7",
    "@middy/validator": "^2.5.7",
    "aws-sdk": "^2.1185.0",
    "bcryptjs": "^2.4.3",
    "http-errors-enhanced": "^0.7.0",
    "jsonwebtoken": "^8.5.1",
    "lambda-multipart-parser": "^1.0.1",
    "middy-middleware-json-error-handler": "^3.0.0",
    "serialize-error": "^8.1.0"
  }
}
